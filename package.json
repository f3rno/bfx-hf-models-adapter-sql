{
  "name": "bfx-hf-models-adapter-sql",
  "version": "1.0.2",
  "description": "SQL adapter for the HF database",
  "main": "index.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/f3rno/bfx-hf-models-adapter-sql.git"
  },
  "bugs": {
    "url": "https://github.com/f3rno/bfx-hf-models-adapter-sql/issues"
  },
  "scripts": {
    "lint": "eslint examples/ test/ lib/ index.js",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test NODE_PATH=lib/ nyc --check-coverage --lines 90 --branches 60 --functions 90 --statements 90 --reporter=lcov --reporter=html mocha -b --recursive",
    "test-without-coverage": "NODE_ENV=test mocha -R spec -b --recursive"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "debug": "^4.1.1",
    "knex": "^0.20.11",
    "lodash": "^4.17.10",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "bfx-hf-ext-plugin-dummy": "^1.0.0",
    "bfx-hf-models": "^2.1.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.2.0"
  }
}
